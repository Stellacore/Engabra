\begin_layout Paragraph
Self Duplex
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
C & =\sigma+\mathcal{T}\\
D & =v+B
\end{align*}

\end_inset


\begin_inset Note Comment
status open

\begin_layout Plain Layout
\begin_inset Formula 
\begin{align*}
C^{2} & =\left(\sigma^{2}+\mathcal{T}^{2}\right)+2\sigma\mathcal{T}\\
D^{2} & =v^{2}+B^{2}+\left(vB+Bv\right)\\
CD & =\sigma v+\sigma B+v\mathcal{T}+B\mathcal{T}\\
DC & =CD
\end{align*}

\end_inset


\end_layout

\end_inset


\begin_inset Formula 
\begin{align*}
CC^{\#} & =\left\langle \sigma^{2}+\mathcal{T}^{2}\right\rangle _{0}+2\left\langle \sigma\mathcal{T}\right\rangle _{3}\\
DD^{\#} & =\left\langle v^{2}+B^{2}\right\rangle _{0}+2\left\langle v\wedge B\right\rangle _{3}\\
CD^{\#} & =\left\langle \sigma v+B\mathcal{T}\right\rangle _{1}+\left\langle \sigma B+v\mathcal{T}\right\rangle _{2}\\
DC^{\#} & =\left\langle \sigma v+B\mathcal{T}\right\rangle _{1}+\left\langle \sigma B+v\mathcal{T}\right\rangle _{2}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
MM^{\#} & =\left\langle CC^{\#}+DD^{\#}\right\rangle _{C}+\left\langle CD^{\#}+DC^{\#}\right\rangle _{D}
\end{align*}

\end_inset


\end_layout

\begin_layout Paragraph
Opposites
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
C^{\circ} & =\sigma-\mathcal{T}\\
D^{\circ} & =-v+B
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
C^{\circ2} & =\left\langle \sigma^{2}+\mathcal{T}^{2}\right\rangle _{0}-2\left\langle \sigma\mathcal{T}\right\rangle _{3}=C^{\dagger2}\\
D^{\circ2} & =\left\langle v^{2}+B^{2}\right\rangle _{0}-2\left\langle v\wedge B\right\rangle _{3}=D^{\dagger2}\\
CD=DC & =-\left\langle \sigma v+B\mathcal{T}\right\rangle _{1}+\left\langle \sigma B+v\mathcal{T}\right\rangle _{2}=\left(CD\right)^{\circ}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
CC^{\circ} & =\left\langle \sigma^{2}+\mathcal{T}^{2}\right\rangle _{0}-2\left\langle \sigma\mathcal{T}\right\rangle _{3}\\
DD^{\circ} & =\left\langle v^{2}+B^{2}\right\rangle _{0}-2\left\langle v\wedge B\right\rangle _{3}\\
CD^{\circ} & =-\left\langle \sigma v-B\mathcal{T}\right\rangle _{1}+\left\langle \sigma B-v\mathcal{T}\right\rangle _{2}\\
DC^{\circ} & =\left\langle \sigma v-B\mathcal{T}\right\rangle _{1}+\left\langle \sigma B-v\mathcal{T}\right\rangle _{2}=\left(CD^{\circ}\right)^{\circ}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
MM^{\circ} & =\left\langle CC^{\circ}+DD^{\circ}\right\rangle _{C}+2\left\langle CD^{\circ}\right\rangle _{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Paragraph
Reverses
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
C^{\dagger} & =\sigma-\mathcal{T}\\
D^{\dagger} & =v-B
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
CC^{\dagger} & =\left\langle \sigma^{2}+\mathcal{T}^{2}\right\rangle _{0}-2\left\langle \sigma\mathcal{T}\right\rangle _{3}\\
DD^{\dagger} & =\left\langle v^{2}+B^{2}\right\rangle _{0}-2\left\langle v\wedge B\right\rangle _{3}\\
CD^{\dagger} & =\left\langle \sigma v-B\mathcal{T}\right\rangle _{1}+\left\langle -\sigma B+v\mathcal{T}\right\rangle _{2}\\
DC^{\dagger} & =\left\langle \sigma v-B\mathcal{T}\right\rangle _{1}+\left\langle \sigma B-v\mathcal{T}\right\rangle _{2}=-CD^{\dagger}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
MM^{\dagger} & =\left\langle CC^{\dagger}+DD^{\dagger}\right\rangle _{C}+2\left\langle CD^{\dagger}\right\rangle _{1}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
MM^{\dagger}=\left\Vert M\right\Vert ^{2} & =\left\langle \left\Vert C\right\Vert ^{2}+\left\Vert M\right\Vert ^{2}\right\rangle _{0}+2\left(\left\langle CD^{\dagger}\right\rangle _{1}-\left\langle \sigma\mathcal{T}+v\wedge B\right\rangle _{3}\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Paragraph
Redirects
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
C^{\ddagger} & =\sigma+\mathcal{T}\\
D^{\ddagger} & =-v-B=-D
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
CC^{\ddagger} & =\left\langle \sigma^{2}+\mathcal{T}^{2}\right\rangle _{0}+2\left\langle \sigma\mathcal{T}\right\rangle _{3}\\
DD^{\ddagger} & =\left\langle v^{2}+B^{2}\right\rangle _{0}+2\left\langle v\wedge B\right\rangle _{3}\\
CD^{\ddagger} & =-\left\langle \sigma v+B\mathcal{T}\right\rangle _{1}-\left\langle \sigma B+v\mathcal{T}\right\rangle _{2}=-CD=-DC\\
DC^{\ddagger} & =-\left\langle \sigma v+B\mathcal{T}\right\rangle _{1}-\left\langle \sigma B+v\mathcal{T}\right\rangle _{2}=-DC=-CD
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
MM^{\ddagger} & =\left\langle CC^{\ddagger}+DD^{\ddagger}\right\rangle _{C}+\left\langle 2CD\right\rangle _{D}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
MM^{\ddagger} & =\left|M\right|^{2}=\left\langle \left|C\right|^{2}+\left|D\right|^{2}\right\rangle _{C}+\left\langle 2CD\right\rangle _{D}
\end{align*}

\end_inset


\end_layout

