\begin_inset Note Comment
status collapsed

\begin_layout Plain Layout
\begin_inset Formula 
\[
MM=\left(\sigma+v+B+\mathcal{T}\right)\left(\sigma+v+B+\mathcal{T}\right)
\]

\end_inset


\begin_inset Formula 
\begin{align*}
MM & =\sigma\sigma+\sigma v+\sigma B+\sigma\mathcal{T}\\
 & +\sigma v+vv+vB+v\mathcal{T}\\
 & +\sigma B+Bv+BB+B\mathcal{T}\\
 & +\sigma\mathcal{T}+v\mathcal{T}+B\mathcal{T}+\mathcal{T}\mathcal{T}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
MM & =\sigma\sigma+vv+BB+\mathcal{T}\mathcal{T}\\
 & +\sigma v+\sigma v+B\mathcal{T}+B\mathcal{T}\\
 & +\sigma B+\sigma B+v\mathcal{T}+v\mathcal{T}\\
 & +\sigma\mathcal{T}+\sigma\mathcal{T}\\
 & +vB+Bv
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
MM & =\sigma\sigma+vv+BB+\mathcal{T}\mathcal{T}\\
 & +2\sigma v+2B\mathcal{T}\\
 & +2\sigma B+2v\mathcal{T}\\
 & +2\sigma\mathcal{T}+\left(vB+Bv\right)
\end{align*}

\end_inset


\end_layout

\end_inset


\begin_inset Formula 
\begin{align*}
MM & =\left\langle \sigma^{2}+v^{2}+B^{2}+\mathcal{T}^{2}\right\rangle _{0}\\
 & +2\left\langle \sigma v+B\mathcal{T}\right\rangle _{1}\\
 & +2\left\langle \sigma B+v\mathcal{T}\right\rangle _{2}\\
 & +2\left\langle \sigma\mathcal{T}+2v\wedge B\right\rangle _{3}
\end{align*}

\end_inset


\end_layout

