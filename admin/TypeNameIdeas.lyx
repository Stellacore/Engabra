\begin_layout Section
General Product
\end_layout

\begin_layout Standard
For MultiVectors,
\begin_inset Formula 
\[
M=\alpha+a+A+\mathcal{A}
\]

\end_inset

and
\begin_inset Formula 
\[
N=\beta+b+B+\mathcal{B}
\]

\end_inset

the product may be expressed as
\begin_inset Formula 
\[
MN=\left(\alpha+a+A+\mathcal{A}\right)\left(\beta+b+B+\mathcal{B}\right)
\]

\end_inset


\begin_inset Formula 
\begin{align*}
MN & =\alpha\beta+\alpha b+\alpha B+\alpha\mathcal{B}\\
 & +\beta a+ab+aB+a\mathcal{B}\\
 & +\beta A+Ab+AB+A\mathcal{B}\\
 & +\beta\mathcal{A}+b\mathcal{A}+B\mathcal{A}+\mathcal{A}\mathcal{B}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
MN & =\alpha\beta+\mathcal{A}\mathcal{B}\\
 & +\alpha b+\beta a+A\mathcal{B}+B\mathcal{A}\\
 & +\alpha B+\beta A+a\mathcal{B}+b\mathcal{A}\\
 & +\alpha\mathcal{B}+\beta\mathcal{A}\\
\\
 & +ab+AB\\
 & +aB+Ab
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
MN & =\alpha\beta+\mathcal{A}\mathcal{B}+a\cdot b+A\cdot B\\
 & +\alpha b+\beta a+A\mathcal{B}+B\mathcal{A}+a\cdot B+A\cdot b\\
 & +\alpha B+\beta A+a\mathcal{B}+b\mathcal{A}+a\wedge b+A\otimes B\\
 & +\alpha\mathcal{B}+\beta\mathcal{A}+a\wedge B+A\wedge b
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
If 
\begin_inset Formula $N$
\end_inset

 has grade components that are 
\begin_inset Formula $\pm$
\end_inset

 the value of the same grade in 
\begin_inset Formula $M$
\end_inset

, then several terms disappear e.g.
\begin_inset Formula 
\begin{align*}
a\wedge b & =0\\
A\otimes B & =0\\
a\cdot B+A\cdot b & =0
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
MM & =\alpha\beta+\mathcal{A}\mathcal{B}+a\cdot b+A\cdot B\\
 & +\alpha b+\beta a+A\mathcal{B}+B\mathcal{A}\\
 & +\alpha B+\beta A+a\mathcal{B}+b\mathcal{A}\\
 & +\alpha\mathcal{B}+\beta\mathcal{A}+a\wedge B+A\wedge b
\end{align*}

\end_inset

In 
\begin_inset Quotes eld
\end_inset

a-b
\begin_inset Quotes erd
\end_inset

 order:
\begin_inset Formula 
\begin{align*}
MM & =\alpha\beta+a\cdot b+\mathcal{A}\mathcal{B}+A\cdot B\\
 & +\alpha b+a\beta+A\mathcal{B}+\mathcal{A}B\\
 & +\alpha B+a\mathcal{B}+A\beta+\mathcal{A}b\\
 & +\alpha\mathcal{B}+a\wedge B+A\wedge b+\mathcal{A}\beta
\end{align*}

\end_inset

neg scalar, 
\begin_inset Formula $\alpha$
\end_inset

,
\begin_inset Formula 
\begin{align*}
MM & =-\alpha\beta+a\cdot b+\mathcal{A}\mathcal{B}+A\cdot B\\
 & +A\mathcal{B}+\mathcal{A}B\\
 & +a\mathcal{B}+\mathcal{A}b\\
 & +a\wedge B+A\wedge b
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
neg vector, 
\begin_inset Formula $a$
\end_inset

,
\begin_inset Formula 
\begin{align*}
MM & =\alpha\beta-a\cdot b+\mathcal{A}\mathcal{B}+A\cdot B\\
 & +A\mathcal{B}+\mathcal{A}B\\
 & +\alpha B+A\beta\\
 & +\alpha\mathcal{B}+\mathcal{A}\beta
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
neg bivector, 
\begin_inset Formula $A$
\end_inset

,
\begin_inset Formula 
\begin{align*}
MM & =\alpha\beta+a\cdot b-\mathcal{A}\mathcal{B}+A\cdot B\\
 & +\alpha b+a\beta\\
 & +a\mathcal{B}+\mathcal{A}b\\
 & +\alpha\mathcal{B}+\mathcal{A}\beta
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
neg trivector, 
\begin_inset Formula $\mathcal{A}$
\end_inset

,
\begin_inset Formula 
\begin{align*}
MM & =\alpha\beta+a\cdot b+\mathcal{A}\mathcal{B}-A\cdot B\\
 & +\alpha b+a\beta\\
 & +\alpha B+A\beta\\
 & +a\wedge B+A\wedge b
\end{align*}

\end_inset


\end_layout

\begin_layout Section
Inspection / Characterization Functions
\end_layout

\begin_layout Standard
Forms for expression include:
\end_layout

\begin_layout Itemize
Grade form
\begin_inset Formula 
\[
M=\sigma+v+B+\mathcal{T}
\]

\end_inset


\end_layout

\begin_layout Itemize
Duplex form
\begin_inset Formula 
\begin{align*}
M & =C+D
\end{align*}

\end_inset

where
\begin_inset Formula 
\begin{align*}
C & =\left\langle M\right\rangle _{0}+\left\langle M\right\rangle _{3}\\
D & =\left\langle M\right\rangle _{1}+\left\langle M\right\rangle _{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
Polar form - expressed in terms of duplex expansion,
\begin_inset Formula 
\[
M=\left|M\right|e^{A\hat{D}}
\]

\end_inset


\end_layout

\begin_layout Standard
Functions including:
\end_layout

\begin_layout Itemize
normSq() - squared L2 norm (positive definitive)
\begin_inset Formula 
\[
\left\Vert M\right\Vert ^{2}=\left\langle MM^{\dagger}\right\rangle _{0}=\left\langle M^{\dagger}M\right\rangle _{0}
\]

\end_inset


\end_layout

\begin_layout Itemize
magnitude() - sqrt(scalar(normSq))
\begin_inset Formula 
\[
\left\Vert M\right\Vert =\left\langle MM^{\dagger}\right\rangle _{0}^{\frac{1}{2}}
\]

\end_inset


\end_layout

\begin_layout Itemize
polar() - express multivector in polar form.
 For multivector.
 
\begin_inset Formula $M$
\end_inset

, in 
\begin_inset Quotes eld
\end_inset

duplex decomposition
\begin_inset Quotes erd
\end_inset

 (Complex and Dirplex parts), 
\begin_inset Formula $M=Z+F$
\end_inset

, (e.g.
 
\begin_inset Formula $Z=\alpha+\mathcal{I}\beta$
\end_inset

, and 
\begin_inset Formula $F=\gamma+\mathcal{I}\delta$
\end_inset

) the polar form has a complex amplitude 
\begin_inset Formula $\left|M\right|=\sigma+\mathcal{I}\tau$
\end_inset

, and complex argument, 
\begin_inset Formula $A=\phi+\mathcal{I}\vartheta$
\end_inset

,
\begin_inset Formula 
\begin{align*}
M & =\left|M\right|e^{A\hat{F}}=\left|M\right|\left(\cos A+\hat{F}\sin A\right)=e^{\ln\left|M\right|+A\hat{F}}\\
A & =\arg\left(M\right)\\
\hat{F} & =\frac{F}{\left|F\right|}
\end{align*}

\end_inset

Provided, 
\begin_inset Formula $\left|M\right|\ne0$
\end_inset

 and 
\begin_inset Formula $\left|F\right|\ne0$
\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
amplitude() - in 3D space, this is a complex-like type (complex, dirplex,
 other?)
\begin_inset Formula 
\[
\left|M\right|=\left(MM^{\ddagger}\right)^{\frac{1}{2}}\,;\,\text{in 3D}
\]

\end_inset

or
\begin_inset Formula 
\[
\left|M\right|=\sqrt{MM^{\ddagger}}\,;\,\text{in 3D}
\]

\end_inset


\end_layout

\begin_layout Itemize
argument() - dirplex type (directed angle?),
\begin_inset Formula 
\[
\Phi=\arctan\left(\frac{\left|F\right|}{Z}\right)
\]

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
angle() - ? - how/if to define? or grl() ?
\end_layout

\end_deeper
\end_deeper
\begin_layout Itemize
inverse() - algebraic inverse (when/if it exists)
\begin_inset Formula 
\[
M^{-1}=\frac{M^{\ddagger}}{MM^{\ddagger}}=\frac{M^{\ddagger}}{M^{\ddagger}M}
\]

\end_inset

Provided 
\begin_inset Formula $MM^{\dagger}\ne0$
\end_inset

, which, for 
\begin_inset Formula $M=Z+F$
\end_inset

, requires 
\begin_inset Formula $F^{2}\ne Z^{2}$
\end_inset

 (i.e.
 where 
\begin_inset Formula $Z$
\end_inset

 is Complex and 
\begin_inset Formula $F$
\end_inset

 is Dirplex type).
\end_layout

